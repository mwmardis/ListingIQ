# ListingIQ Environment Variables
# Copy to .env and fill in your values

# Database
DATABASE_URL=sqlite:///listingiq.db

# Email alerts (SMTP)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
ALERT_EMAIL_FROM=your-email@gmail.com
ALERT_EMAIL_TO=investor@example.com

# SMS alerts (Twilio)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_FROM_NUMBER=
ALERT_SMS_TO=

# Webhook alerts
WEBHOOK_URLS=

# Scraper settings
SCRAPER_INTERVAL_MINUTES=60
